{
  "name": "vipauto-crm-frontend",
  "version": "1.0.0",
  "description": "Frontend для CRM системы автосервиса VIPauto",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "pwa:generate": "vite-plugin-pwa",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/static"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@tanstack/react-query": "^5.8.4",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "@supabase/supabase-js": "^2.38.4",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "react-hot-toast": "^2.4.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.20",
    "recharts": "^2.8.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "@headlessui/react": "^1.7.17",
    "framer-motion": "^10.16.16",
    "react-helmet-async": "^2.0.4",
    "workbox-window": "^7.0.0",
    "idb": "^7.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-window": "^1.8.8",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-pwa": "^0.17.4",
    "vite": "^5.0.10",
    "vite-plugin-pwa": "^0.17.4",
    "vite-bundle-analyzer": "^0.9.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "c8": "^8.0.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "jsdom": "^23.0.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "crm",
    "autocare",
    "react",
    "typescript",
    "vite",
    "pwa"
  ],
  "author": "VIPauto Team",
  "license": "MIT"
}